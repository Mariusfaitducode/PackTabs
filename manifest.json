{
    "manifest_version": 2,
    "name": "PackTabs",
    "version": "1.0",
    "description": "Save and reopen link packs in Firefox.",
    "icons": {
      "48": "./icons/link-48.png"
    },
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
      "scripts": ["./background.js"]
    },
    "browser_action": {
      "default_icon": "./icons/link-48.png",
      "default_popup": "./popup.html"
    }
  }
  